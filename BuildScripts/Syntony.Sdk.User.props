***********************************************************************************************
Function: set the default user name and E-mail
Imported by:        '$(BuildRootName)' => build.root
    Imported by:    'Syntony.Sdk.DefaultItems.props'
***********************************************************************************************
Properties defined
    IsUserPropsFileImported         ... 'true'
    Author                          ... '$(USERNAME)'
    AuthorLongName                  ... '$(USERNAME)'
    AuthorEmail                     ... '$(CompanyEmail)'
***********************************************************************************************
-->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <PropertyGroup Label="Imported">
        <IsUserPropsFileImported>true</IsUserPropsFileImported>
    </PropertyGroup>

    <PropertyGroup Label="User settings">
        <Author>$(USERNAME)</Author>
        <AuthorLongName>$(USERNAME)</AuthorLongName>
        <AuthorEmail>$(CompanyEmail)</AuthorEmail>
    </PropertyGroup>

</Project>
